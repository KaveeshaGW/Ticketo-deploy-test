generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// User table
model User {
  id        String @id @default(uuid()) // Primary Key
  email     String @unique
  password  String
  firstName String
  userType  String
  token     String @default("")
  // Relationships
  // employees      Employee[]
  // dependents     Dependent[]
  // wallets        Wallet[]
  // seasonCards    SeasonCard[]
  // tickets        Ticket[]
}
